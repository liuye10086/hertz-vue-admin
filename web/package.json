{
  "name": "gin-vue-admin",
  "version": "2.5.4",
  "type": "module",
  "private": true,
  "scripts": {
    "serve": "cross-env SASS_SILENCE_DEPRECATIONS=all vite --host --mode development",
    "build": "cross-env SASS_SILENCE_DEPRECATIONS=all vite build --mode production",
    "limit-build": "npm install increase-memory-limit-fixbug cross-env -g && npm run fix-memory-limit && node ./limit && npm run build",
    "preview": "cross-env SASS_SILENCE_DEPRECATIONS=all vite preview",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.7.2",
    "core-js": "^3.37.1",
    "echarts": "^5.5.0",
    "element-plus": "^2.8.4",
    "highlight.js": "^11.9.0",
    "marked": "^12.0.2",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.1.7",
    "qs": "^6.12.0",
    "quill": "^1.3.7",
    "screenfull": "^6.0.2",
    "spark-md5": "^3.0.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/compiler-sfc": "^3.4.27",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
